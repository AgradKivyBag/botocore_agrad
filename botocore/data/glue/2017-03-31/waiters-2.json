{
  "version": 2,
  "waiters": {
    "JobRunSucceeded": {
      "delay": 30,
      "operation": "GetJobRun",
      "maxAttempts": 60,
      "acceptors": [
        {
          "state": "success",
          "matcher": "path",
          "argument": "JobRun.JobRunState",
          "expected": "SUCCEEDED"
        },
        {
          "state": "failure",
          "matcher": "path",
          "argument": "JobRun.JobRunState",
          "expected": "FAILED"
        },
        {
          "state": "failure",
          "matcher": "path",
          "argument": "JobRun.JobRunState",
          "expected": "STOPPING"
        },
        {
          "state": "failure",
          "matcher": "path",
          "argument": "JobRun.JobRunState",
          "expected": "STOPPED"
        },
        {
          "state": "failure",
          "matcher": "path",
          "argument": "JobRun.JobRunState",
          "expected": "TIMEOUT"
        }
      ]
    }
  }
}
